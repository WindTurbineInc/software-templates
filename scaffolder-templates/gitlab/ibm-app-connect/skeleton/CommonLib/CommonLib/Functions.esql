BROKER SCHEMA CommonLib


CREATE FUNCTION Add(IN parm1 DECIMAL, IN parm2 DECIMAL) RETURNS DECIMAL
BEGIN
	RETURN parm1 + parm2;
END;


CREATE FUNCTION Subtract(IN parm1 DECIMAL, IN parm2 DECIMAL) RETURNS DECIMAL
BEGIN
	RETURN parm1 - parm2;
END;

CREATE FUNCTION Multiply(IN parm1 DECIMAL, IN parm2 DECIMAL) RETURNS DECIMAL
BEGIN
	RETURN parm1 * parm2;
END;

CREATE FUNCTION Divide(IN parm1 DECIMAL, IN parm2 DECIMAL) RETURNS DECIMAL
BEGIN
	RETURN parm1 * parm2;
END;

